<StyleList>
 
   <Style name=".dummy" >
      <StyleAttribute name = "fontSize" value = "1pt" />
      <StyleAttribute name = "border" value = "none" />
      <StyleAttribute name = "textColor" value = "transparent" />   
      <StyleAttribute name = "backgroundColor" value = "transparent" />   
   </Style>

   <Style name="Window.common" >
      <StyleAttribute name = "forceDefaultSettings" value = "1" />
   </Style>
    <Style name="Window.dialog" >
      <StyleAttribute name = "windowType" value = "modal" />
   </Style>

   <Style name="Folder.accordion">
      <StyleAttribute name="position" value="accordion" />
   </Style>
   
   <Style name=":query" >
      <StyleAttribute name = "backgroundColor" value = "yellow" />
   </Style>
   
   <Style name=".mandatory">
      <StyleAttribute name="backgroundColor" value="pink" />
   </Style>
   <Style name=".invalid">
      <StyleAttribute name="backgroundColor" value="red" />
   </Style>
   

</StyleList>